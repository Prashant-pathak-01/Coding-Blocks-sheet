import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int max = Integer.MIN_VALUE;
        for(int i=0; i<n; i++){
            int temp = sc.nextInt();
            max = Math.max(temp,max);
        }
        System.out.println(max);

    }
}